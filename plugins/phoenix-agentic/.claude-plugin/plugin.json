{
  "name": "phoenix-agentic",
  "version": "1.0.0",
  "description": "Core agentic system plugin for 2 Acre Studios - integrates MCP servers, voice mode, memory, and distributed execution",
  "author": "Marc Shade <marc@2acrestudios.com>",
  "repository": "https://github.com/marc-shade/claude-code",
  "keywords": ["agentic", "mcp", "voice", "memory", "distributed", "phoenix"],
  "commands": ["system-status", "phoenix-mode", "ember-check"],
  "agents": ["orchestrator", "researcher", "builder"],
  "skills": ["CORE", "voice-integration", "memory-management"],
  "hooks": ["session-start", "pre-tool-use", "post-tool-use"],
  "mcpServers": [
    "enhanced-memory",
    "voice-mode",
    "agent-runtime-mcp",
    "sequential-thinking",
    "arduino-surface",
    "safla-mcp"
  ],
  "settings": {
    "voiceEnabled": true,
    "memoryEnabled": true,
    "clusterEnabled": true,
    "productionOnly": true
  }
}
